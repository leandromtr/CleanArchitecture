@model  IEnumerable<CleanArchitecture.Application.ViewModels.CourseViewModel>

<div class="container cody-content">
    <div class="row cody-content">
        @foreach(var course in Model)
        {
            <div class="course-image-container">
                <div class="course-image" style="background-image:url(@course.ImageURL)"></div>
            </div>
            <span>@course.Name</span>
        }
    </div>
</div>